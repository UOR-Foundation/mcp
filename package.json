{
  "name": "mcp-uor-server",
  "version": "0.1.0",
  "description": "Model Context Protocol (MCP) - UOR Implementation",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "build": "vite build",
    "build:tsc": "tsc",
    "build:client": "cp -r public/ dist/public/",
    "start": "node dist/mcp-uor-server.cjs.js",
    "dev": "vite --port 3000",
    "dev:client": "npx http-server public/",
    "deploy": "gh-pages -d public",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:core": "NODE_OPTIONS=--experimental-vm-modules jest src/core/tests",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "lint": "eslint --ext .ts,.js src/",
    "lint:fix": "eslint --ext .ts,.js src/ --fix",
    "format": "prettier --write \"src/**/*.{ts,js}\"",
    "prepare": "husky install",
    "prepare-gh-pages": "npm run build && npm run build:client && node scripts/deploy-to-github-pages.sh",
    "analyze": "vite build --mode analyze",
    "docs": "typedoc",
    "dedupe": "npm dedupe",
    "audit": "npm audit fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UOR-Foundation/mcp.git"
  },
  "keywords": [
    "mcp",
    "uor",
    "model-context-protocol"
  ],
  "author": "UOR Foundation",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UOR-Foundation/mcp/issues"
  },
  "homepage": "https://github.com/UOR-Foundation/mcp#readme",
  "dependencies": {
    "@octokit/rest": "^19.0.13",
    "@types/cors": "^2.8.17",
    "@types/json-schema": "^7.0.15",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-oauth2-jwt-bearer": "^1.5.0",
    "ipfs-http-client": "^60.0.1",
    "json-schema": "^0.4.0",
    "jsonschema": "^1.4.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^20.17.32",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vitejs/plugin-react": "^4.4.1",
    "axios": "^1.9.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.6",
    "gh-pages": "^6.0.0",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.12",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.1.6",
    "typescript-eslint": "^8.31.1",
    "vite": "^6.3.3",
    "vite-plugin-dts": "^4.5.3",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
